{
	"env": {
		"browser": true,
		"amd": true,
		"es6": true,
		"mocha": true,
		"jquery": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": ["airbnb", "prettier", "prettier/react"],
	"rules": {
		"arrow-parens": 0,
		"camelcase": 0,
		"dot-notation": 0,
		"eqeqeq": 0,
		"func-style": 0,
		"import/no-extraneous-dependencies": 0,
		"jsx-a11y/anchor-has-content": 0,
		"jsx-a11y/anchor-is-valid": 0,
		"jsx-a11y/click-events-have-key-events": 0,
		"jsx-a11y/label-has-for": 0,
		"jsx-a11y/no-static-element-interactions": 0,
		"no-alert": 1,
		"no-case-declarations": 0,
		"no-confusing-arrow": 0,
		"no-console": 1,
		"no-debugger": 1,
		"no-nested-ternary": 0,
		"no-param-reassign": [1, { "props": true, "ignorePropertyModificationsFor": ["data", "tooltipItem"] }],
		"no-plusplus": 0,
		"no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
		"no-underscore-dangle": 0,
		"radix": 0,
		"react/forbid-prop-types": 0,
		"react/jsx-filename-extension": 0,
		"react/no-multi-comp": 0,
		"react/prefer-stateless-function": 0,
		"react/prop-types": [1, { "ignore": ["location", "history"] }],
		"react/require-default-props": 0,
		"spaced-comment": 0,
        "no-unused-vars": [1, { "ignoreRestSiblings": true, "caughtErrors": "none" }]
	},
	"globals": {
		"expect": true,
		"tableau": true
	}
}
